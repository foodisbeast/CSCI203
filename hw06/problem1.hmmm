0       read    r1              # Get an integer from the user
                                # and hold it in register r1
1       write   r1              # write out (print) the value of r1
2       addn    r1, 1           # Add 1 to the value in register r1
                                # in other words, r1 = r1 + 1
3       jumpn   1               # Jump to location 1 next
4       halt                    # This will never happen!
                                # Press Control-C to stop the program!
